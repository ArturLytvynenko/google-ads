              "SHOPPING_PERFORMANCE_REPORT",
              "PRODUCT_PARTITION_REPORT",
              "PARENTAL_STATUS_PERFORMANCE_REPORT",
              "PLACEHOLDER_REPORT",
              "AD_CUSTOMIZERS_FEED_ITEM_REPORT",
