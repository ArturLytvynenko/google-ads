          "label": "Customer ID",
          "name": "clientCustomerId"
        },
        {
          "widget-type": "textbox",
          "label": "Start Date",
          "name": "startDate"
        },
        {
          "widget-type": "textbox",
          "label": "End Date",
          "name": "endDate"
        }
      ]
    },
    {
      "label": "Advanced",
      "properties": [
        {
          "widget-type": "toggle",
          "label": "Include Report Summary",
          "name": "includeReportSummary",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "true"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Use Raw Enum Values",
          "name": "useRawEnumValues",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "false"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Include Zero Impressions",
          "name": "includeZeroImpressions",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "false"
          }
        },
        {
          "name": "reportFields",
          "label": "Report Fields",
          "widget-type": "dsv",
          "widget-attributes": {
            "delimiter": ","
          }
        }
      ]
    }
